{
    "name": "swisnl/laravel-api",
    "type": "library",
    "description": "API in Laravel",
    "keywords": [
        "swisnl",
        "laravel-api"
    ],
    "homepage": "https://github.com/swisnl/laravel-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Dylan",
            "email": ":author_email",
            "homepage": "https://www.swis.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "swisnl\\laravel-api\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "swisnl\\laravel-api\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
